<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>CSV Lookup Results</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="{% if dashboard is same as(false) %}../{% endif %}_css/bootstrap-5.0.2.min.css" rel="stylesheet" type="text/css">
    <link href="{% if dashboard is same as(false) %}../{% endif %}_css/styles.css" rel="stylesheet" type="text/css">
    <script src="{% if dashboard is same as(false) %}../{% endif %}_js/fontawesome-5.15.3.min.js" type="text/javascript"></script>
</head>
<body>
    <header>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <span class="fw-bold fs-2">csv-lookup results</span>
                    {% include 'partials/breadcrumbs.html.twig' %}
                </div>
            </div>
        </div>
    </header>

    {{ block('body') }}

    <footer>
        {% include 'partials/query-type-legend.html.twig' %}

        <div class="row col small">
            <p class="small">Generated by <a href="https://github.com/thor-juhasz/csv-lookup" target="_top">csv-lookup {{ version }}</a> using <a href="https://secure.php.net/" target="_top">PHP 8.0.8</a> (powered by <a href="https://twig.symfony.com/" target="_top">Twig</a>) at {{ timestamp }}.</p>
        </div>

        <a href="#" title="Back to top" id="toplink" class="position-fixed bottom-0 end-0 m-3">
            <span class="fa-layers fa-fw fa-2x">
                <i class="fas fa-circle text-dark"></i>
                <i class="fas fa-chevron-up text-light" data-fa-transform="shrink-5 up-0.5"></i>
            </span>
        </a>
    </footer>

    <script src="{% if dashboard is same as(false) %}../{% endif %}_js/bootstrap-5.0.2.bundle.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        let tooltipTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="tooltip"]'));
        tooltipTriggerList.map(function (tooltipTriggerEl) {
            return new bootstrap.Tooltip(tooltipTriggerEl)
        });
        let popoverTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="popover"]'))
        popoverTriggerList.map(function (popoverTriggerEl) {
            return new bootstrap.Popover(popoverTriggerEl, { trigger: 'hover focus' })
        });
    </script>
</body>
</html>
